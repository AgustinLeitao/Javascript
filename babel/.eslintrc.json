{
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": { "es6": true },         
    "plugins": ["prettier"],
    "rules": {
        "no-unused-vars": "warn",
        "prettier/prettier": "error"
  }
}